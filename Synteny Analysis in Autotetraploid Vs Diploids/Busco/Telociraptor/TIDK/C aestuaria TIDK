# Activate the TIDK environment if not already activated
conda activate tidk_env

# Run the command 
tidk search -s TTAGGG -o Cochlearia_austeria -d /gpfs01/home/mzywj2 --extension tsv /gpfs01/home/mzywj2/C_aestuaria.fasta

# In order to use this as an input file for chromsyn, the tsv file must be converted into a csv file
awk 'BEGIN {FS="\t"; OFS=","} {$1=$1} 1' Cochlearia_austeria_telomeric_repeat_windows.tsv > C_aestuaria_telomeric_repeat_windows.csv
