# Activate the TIDK environment if not already activated
conda activate tidk_env

# Run the command 
tidk search -s TTAGGG -o Cochlearia_exc -d /gpfs01/home/mzywj2 --extension tsv /gpfs01/home/mzywj2/C_excelsa_V5.fa.masked.fa

# In order to use this as an input file for chromsyn, the tsv file must be converted into a csv file
awk 'BEGIN {FS="\t"; OFS=","} {$1=$1} 1' Cochlearia_exc_telomeric_repeat_windows.tsv > C_excelsa_telomeric_repeat_windows.csv

